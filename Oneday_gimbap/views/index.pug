extends layout

block content
	section#main 
		article.pos
			for i in [1,2,3,4,5,6,7,8,9]
				if i%3 === 1
					div.trow
						for j in [0,1,2]
							div.tdata #{i+j}		
	script.
		document.querySelector("article.pos")
		.addEventListener("click",(e)=>{
			const data = e.target 
			if(data.className.includes("tdata")) {
				let o_table = data.innerText
				//- alert(o_table)
				document.location.href=`/order?o_table=${o_table}`
			}						
		})
